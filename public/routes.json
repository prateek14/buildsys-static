{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "defaultHeaders": {
    "accept-encoding": "gzip, compress, br",
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'",
    "cache-control": "must-revalidate, max-age=31536000",
    "x-dns-prefetch-control": ""
  }
}